{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "target": "es5",
    "outDir": "build/tsc_output",
    "lib": [
      "dom",
      "es5",
      "es2015",
      "scripthost"
    ],
    "sourceMap": true,
    "alwaysStrict": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "include": [
    "src"
  ]
}
